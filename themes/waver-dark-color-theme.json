      }
    },
    {
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "foreground": "#FFC18F"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#484C53"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#D8B6FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#B0C1FF"
      }
    },
    {
      "scope": "meta.separator",
