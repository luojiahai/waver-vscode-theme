      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "scope": [
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#77777777"
      }
    },
    {
      "scope": [
        "punctuation.support.type.property-name"
      ],
      "settings": {
        "foreground": "#D69AAE77"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#D69AAE"
      }
    },
    {
      "scope": [
        "property",
        "meta.property-name",
        "meta.object-literal.key",
