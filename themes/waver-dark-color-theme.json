  "semanticHighlighting": true,
  "semanticTokenColors": {
    "namespace": "#6EA6C2",
    "property": "#6EA6C2",
    "interface": "#6EC2C2",
    "type": "#6EC2C2",
    "class": "#6EC2C2"
  },
  "tokenColors": [
    {
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "#768085dd"
      }
    },
    {
      "scope": [
        "delimiter.bracket",
        "delimiter",
        "invalid.illegal.character-not-allowed-here.html",
        "keyword.operator.rest",
        "keyword.operator.spread",
        "keyword.operator.type.annotation",
        "keyword.operator.relational",
        "keyword.operator.assignment",
        "keyword.operator.type",
        "meta.brace",
        "meta.tag.block.any.html",
        "meta.tag.inline.any.html",
        "meta.tag.structure.input.void.html",
        "meta.type.annotation",
        "meta.embedded.block.github-actions-expression",
        "storage.type.function.arrow",
        "meta.objectliteral.ts",
        "punctuation",
        "punctuation.definition.string.begin.html.vue",
        "punctuation.definition.string.end.html.vue"
      ],
      "settings": {
        "foreground": "#ABB0B2"
      }
    },
    {
      "scope": [
        "constant",
        "entity.name.constant",
        "variable.language",
        "meta.definition.variable"
      ],
      "settings": {
        "foreground": "#C26E8A"
      }
    },
    {
      "scope": [
        "entity",
        "entity.name"
      ],
      "settings": {
        "foreground": "#C2A66E"
      }
    },
    {
      "scope": "variable.parameter.function",
      "settings": {
        "foreground": "#ccd6dbee"
      }
    },
    {
      "scope": [
        "entity.name.tag",
        "tag.html"
      ],
      "settings": {
        "foreground": "#6EA6C2"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#C2A66E"
      }
    },
    {
      "scope": [
        "keyword",
        "storage.type.class.jsdoc",
        "punctuation.definition.template-expression"
      ],
      "settings": {
        "foreground": "#6EA6C2"
      }
    },
    {
      "scope": [
        "storage",
        "storage.type",
        "support.type.builtin",
        "constant.language.undefined",
        "constant.language.null",
        "constant.language.import-export-all.ts"
      ],
      "settings": {
        "foreground": "#6EA6C2"
      }
    },
    {
      "scope": [
        "text.html.derivative",
        "storage.modifier.package",
        "storage.modifier.import",
        "storage.type.java"
      ],
      "settings": {
        "foreground": "#ccd6dbee"
      }
    },
    {
      "scope": [
        "string",
        "string punctuation.section.embedded source",
        "attribute.value"
      ],
      "settings": {
        "foreground": "#ABB0B2"
      }
    },
    {
      "scope": [
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#ABB0B290"
      }
    },
    {
      "scope": [
        "punctuation.support.type.property-name"
      ],
      "settings": {
        "foreground": "#6EA6C290"
      }
    },
    {
      "scope": "support",
      "settings": {
        "foreground": "#6EA6C2"
      }
    },
    {
      "scope": [
        "property",
        "meta.property-name",
        "meta.object-literal.key",
        "entity.name.tag.yaml",
        "attribute.name"
      ],
      "settings": {
        "foreground": "#6EA6C2"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name",
        "invalid.deprecated.entity.other.attribute-name.html"
      ],
      "settings": {
        "foreground": "#C26E8A"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#C26E8A"
      }
    },
    {
      "scope": [
        "support.type.primitive",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#6EC2C2"
      }
    },
    {
      "scope": "namespace",
      "settings": {
        "foreground": "#6EA6C2"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment.compound",
        "meta.var.expr.ts"
      ],
      "settings": {
        "foreground": "#C2A66E"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF9999"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF9999"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF9999"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FF9999"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#17191a"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#FF9999"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#ABB0B2"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#C26E6E"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
        "string.regexp constant.character.escape",
        "string.regexp source.ruby.embedded",
        "string.regexp string.regexp.arbitrary-repetition"
      ],
      "settings": {
        "foreground": "#ABB0B2"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "foreground": "#C2A66E"
      }
    },
    {
      "scope": [
        "support.constant"
      ],
      "settings": {
        "foreground": "#C26E8A"
      }
    },
    {
      "scope": [
        "keyword.operator.quantifier.regexp",
        "constant.numeric",
        "number"
