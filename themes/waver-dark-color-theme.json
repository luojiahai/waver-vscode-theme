      "settings": {
        "foreground": "#3EA0C3"
      }
    },
    {
      "scope": [
        "variable",
        "identifier"
      ],
      "settings": {
        "foreground": "#3EA0C3"
      }
    },
    {
      "scope": [
        "support.type.primitive",
        "entity.name.type"
      ],
      "settings": {
        "foreground": "#D69363"
      }
    },
    {
      "scope": "namespace",
      "settings": {
        "foreground": "#D69AAE"
      }
    },
    {
      "scope": [
        "keyword.operator",
        "keyword.operator.assignment.compound",
        "meta.var.expr.ts"
      ],
      "settings": {
        "foreground": "#CF8B88"
      }
    },
    {
      "scope": "invalid.broken",
      "settings": {
