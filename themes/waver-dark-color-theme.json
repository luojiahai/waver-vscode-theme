      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "scope": "string.regexp constant.character.escape",
      "settings": {
        "foreground": "#D6AD65"
      }
    },
    {
      "scope": [
        "support.constant"
      ],
      "settings": {
        "foreground": "#D69363"
      }
    },
    {
      "scope": [
        "keyword.operator.quantifier.regexp",
