    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#C28A6E"
      }
    },
    {
      "scope": [
        "markup.heading",
        "markup.heading entity.name"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#6EA6C2"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#6EC2C2"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#ccd6dbee"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#ccd6dbee"
      }
    },
    {
      "scope": "markup.raw",
      "settings": {
        "foreground": "#6EA6C2"
      }
    },
    {
      "scope": [
        "markup.deleted",
        "meta.diff.header.from-file",
        "punctuation.definition.deleted"
      ],
      "settings": {
        "foreground": "#FF9999"
      }
    },
    {
      "scope": [
        "markup.inserted",
        "meta.diff.header.to-file",
        "punctuation.definition.inserted"
      ],
      "settings": {
        "foreground": "#80FFAA"
      }
    },
    {
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "foreground": "#FFAA80"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#2b2d2e"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#AA80FF",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#80AAFF"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#80AAFF"
      }
    },
    {
      "scope": "meta.output",
      "settings": {
