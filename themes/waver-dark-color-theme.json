        "foreground": "#FDD7D4"
      }
    },
    {
      "scope": "invalid.deprecated",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FDD7D4"
      }
    },
    {
      "scope": "invalid.illegal",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FDD7D4"
      }
    },
    {
      "scope": "invalid.unimplemented",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#FDD7D4"
      }
    },
    {
      "scope": "carriage-return",
      "settings": {
        "fontStyle": "italic underline",
        "foreground": "#2F3238"
      }
    },
    {
      "scope": "message.error",
      "settings": {
        "foreground": "#FDD7D4"
      }
    },
    {
      "scope": "string variable",
      "settings": {
        "foreground": "#777777"
      }
    },
    {
      "scope": [
        "source.regexp",
        "string.regexp"
      ],
      "settings": {
        "foreground": "#D69363"
      }
    },
    {
      "scope": [
        "string.regexp.character-class",
