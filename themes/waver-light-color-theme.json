        "foreground": "#66b105"
      }
    },
    {
      "scope": [
        "markup.changed",
        "punctuation.definition.changed"
      ],
      "settings": {
        "foreground": "#bb4100"
      }
    },
    {
      "scope": [
        "markup.ignored",
        "markup.untracked"
      ],
      "settings": {
        "foreground": "#e3e5e7"
      }
    },
    {
      "scope": "meta.diff.range",
      "settings": {
        "foreground": "#8f56e4",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#008ac5"
      }
    },
    {
      "scope": "meta.separator",
      "settings": {
