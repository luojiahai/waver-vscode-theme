      "settings": {
        "fontStyle": "bold",
        "foreground": "#008080"
      }
    },
    {
      "scope": "markup.quote",
      "settings": {
        "foreground": "#2e808f"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic",
        "foreground": "#393a34"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold",
        "foreground": "#393a34"
      }
    },
    {
      "scope": "markup.raw",
      "settings": {
        "foreground": "#008080"
      }
    },
